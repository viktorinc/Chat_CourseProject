<Window x:Class="Chat_Course_Project.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chat_Course_Project"
        mc:Ignorable="d"
        Title="Log in" Height="300" Width="300">
    <Grid Background="#FF475059">
        <Button FontSize="20" Background="#FFF37B53" Height="50" Width="100" Margin="152,155,40,65" Click="Reg_Btn">
            Register
        </Button>
        <Button FontSize="20" Background="#FFF37B53" Height="50" Width="100" Margin="47,155,145,65" Click="Log_In_Btn">
            Login
        </Button>
        <TextBox Height="30" Margin="56,31,50,209" Name="usrname"/>
        <TextBox Height="30" Margin="56,83,50,157" Name="pass"/>
    </Grid>
</Window>
